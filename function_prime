function checkprime(input){

var count=0;

for(let i=1;i<=input;i++){

if(input %i === 0){

   count++;
}
}
if ( count ==2){

  return true;

  }

  return false;

  }

  var solution = checkprime(77);  

 if (solution == true){
  
console.log("Is a Prime");
 }
else{
 
console.log("Not a Prime");
 }

